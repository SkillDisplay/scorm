  <metadata>
    <schema>ADL SCORM</schema>
    <schemaversion>1.2</schemaversion>
  </metadata>
  <organizations default="skilldisplay">
    <organization identifier="skilldisplay">
      <title>CMS | ADV | Setting up roles in WordPress</title>
      <item identifier="set_cms__adv__setting_up_roles_in_wordpress_item" identifierref="set_cms__adv__setting_up_roles_in_wordpress_resource">
        <title>CMS | ADV | Setting up roles in WordPress</title>
      </item>
            <item identifier="roles_and_capabilities_in_wordpress_item" identifierref="roles_and_capabilities_in_wordpress_resource">
        <title>Roles and Capabilities in WordPress</title>
      </item>
            <item identifier="user_management_item" identifierref="user_management_resource">
        <title>User Management</title>
      </item>
          </organization>
  </organizations>

  <resources>
    <resource identifier="set_cms__adv__setting_up_roles_in_wordpress_resource" type="webcontent" adlcp:scormtype="sco" href="skills/set_cms__adv__setting_up_roles_in_wordpress.html">
      <file href="skills/set_cms__adv__setting_up_roles_in_wordpress.html"/>
      <dependency identifierref="common_files" />
    </resource>
    <!-- Note that with the addition of run-time calls, we changed the scormtype to SCO.-->
        <resource identifier="roles_and_capabilities_in_wordpress_resource" type="webcontent" adlcp:scormtype="sco" href="skills/roles_and_capabilities_in_wordpress.html">
      <file href="skills/roles_and_capabilities_in_wordpress.html"/>
      <dependency identifierref="common_files" />
    </resource>
        <resource identifier="user_management_resource" type="webcontent" adlcp:scormtype="sco" href="skills/user_management.html">
      <file href="skills/user_management.html"/>
      <dependency identifierref="common_files" />
    </resource>
        <!--The shared files remain as an asset-->
    <resource identifier="common_files" type="webcontent" adlcp:scormtype="asset">
      <file href="shared/scormfunctions.js"/>
      <file href="shared/style.css"/>
      <file href="shared/sdskill.css"/>
      <file href="shared/lato.css"/>
      <file href="shared/font/lato-v23-latin-300.eot"/>
